<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>stdManagement</display-name>
 
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.student.StudentLogin</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/log</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>insertStd</servlet-name>
  	<servlet-class>com.student.InsertStudent</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>insertStd</servlet-name>
  	<url-pattern>/insert</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>updateStudent</servlet-name>
  	<servlet-class>com.student.updateStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>updateStudent</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>deleteStudent</servlet-name>
  	<servlet-class>com.student.DeleteStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>deleteStudent</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>

	